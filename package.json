{
  "name": "cra2-storybook-svg-sass",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "4.1.1",
    "bootswatch": "4.1.1",
    "dayjs": "^1.6.4",
    "prop-types": "15.6.1",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.0.0-next.66cc7a90",
    "reactstrap": "6.0.1"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version react-scripts start",
    "build": "REACT_APP_VERSION=$npm_package_version react-scripts build",
    "build-reactstrap": "node ./utility/build-reactstrap.js",
    "build-screenshots": "node ./integration-screenshots.js",
    "test": "react-scripts test --env=jsdom",
    "style-guide-start": "cross-env NODE_PATH=src start-storybook -p 3002 -c ./style-guide -s ./public",
    "style-guide-build":
      "cross-env NODE_PATH=src build-style-guide -c ./style-guide -o ../react-base-style-guide -s ./public",
    "style-guide-start-build": "./node_modules/.bin/serve -s ../react-base-style-guide",
    "lint": "node node_modules/eslint/bin/eslint.js --max-warnings=0 src",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.4",
    "@storybook/addon-info": "3.4.4",
    "@storybook/addon-links": "3.4.4",
    "@storybook/addons": "3.4.4",
    "@storybook/react": "3.4.4",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "cross-env": "^5.1.6",
    "css-loader": "0.28.11",
    "fs-extra": "6.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "node-sass": "4.9.0",
    "puppeteer": "^1.4.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.1",
    "serve": "^7.1.5",
    "style-loader": "0.21.0",
    "yargs": "11.0.0"
  },
  "browserslist": {
    "development": ["last 2 chrome versions", "last 2 firefox versions", "last 2 edge versions"],
    "production": [">1%", "last 4 versions", "Firefox ESR", "not ie < 11"]
  },
  "eslintConfig": {
    "extends": "./node_modules/eslint-config-react-app/index.js",
    "rules": {
      "no-console": 1,
      "react/default-props-match-prop-types": 1,
      "react/prop-types": 1,
      "react/no-unused-prop-types": 1,
      "react/sort-prop-types": 1,
      "jsx-a11y/href-no-hash": 0
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,css}": ["prettier --write", "git add"],
      "*.js": ["npm run lint", "git add"]
    }
  },
  "prettier": {
    "filepath": "src",
    "jsxBracketSameLine": true,
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  }
}
