{
  "name": "@myorg/app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && yarn build-deploy",
    "test": "react-scripts test --env=jsdom",
    "build-deploy": "node ./utility/build-copy.js && gzip -9 -r deploy/client/static && gzip -9 -r deploy/client/service-worker.js",
    "build-start": "cd deploy && yarn && yarn start",
    "screenshots-build": "node ./utility/build-screenshots.js",
    "tests-build": "node ./utility/build-test.js"
  },
  "dependencies": {
    "prop-types": "15.6.1",
    "react-dom": "16.4.1",
    "react-loadable": "5.4.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.0.0-next.3e165448",
    "react": "16.4.1"
  },
  "devDependencies": {
    "cross-env": "5.2.0",
    "node-sass": "4.9.0",
    "puppeteer": "1.5.0"
  },
  "engines": {
    "node": ">=7.6.0 <10.0.0 "
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  }
}
